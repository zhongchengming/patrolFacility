
page{
  background-color: #F6FAFA;
}
.content-cell{
  background-color: rgb(26, 22, 22);
  margin-top: 20px;
}
.checkTitle{
  font-size:17px;
  padding-left:15px;
  height:50px; 
  line-height:50px;
  background-color:white;
  margin-top: 10px;
  color: rgb(82, 82, 82); 
}
.checkContent{
  font-size:14px;
  padding-left:15px;
  height:50px;
  line-height:50px;
  background-color:white;
  margin-top: 1px
}
.selectRight{
  float: right;
  height: 50px;
  width: 60%;
  background-color:white;
  text-align: center;
   position: relative;
}
.selectLeft{
  float: left;
  height: 50px;
  width: 40%;
  background-color:white;
 
}
.yes{
  position: absolute;
  width: 90px;
  height: 30px;
  line-height: 30px;
  border-style: solid;
  border-width: 1px;
  border-color: rgb(231, 231, 231);
  border-radius: 4px;
  
}
.no{
  position: absolute;
  width: 90px;
  height: 30px;
  line-height: 30px;
   border-style: solid;
  border-width: 1px;
  border-color: rgb(231, 231, 231);
  border-radius: 4px;
  left:100px;
}
.desc{
  width: 100%;
  height: 300px;
  background-color: white;
}
.descTitle{
  width: 50%;
  height: 40px;
  line-height: 40px;
  margin-right: 10px;
}
.desc-cell{
  display: inline-block;
  width: 100%;
  height: 60px;
  margin-bottom: 1px;
}
.view-text{
  height: 55px;
  display: inline-block;
  float: left;
  line-height: 55px;
  font-size: 17px;
  margin-left: 12px;
  width: 70px;
  color: rgb(34, 34, 34)
}
.view-input{
  height: 55px;
  float: left;
  width: calc(100% - 100px);
  line-height: 55px;
  border-style: solid;
  border-width: 0px;
  border-bottom-width: 1px;
  border-bottom-color: rgb(233, 229, 229);
}
.select-left{
    float: left;
    text-align: center;
}
.view-input input{
  height: 55px;
  margin-left: 15px; 
}
.select-Box{
  width: 70PX;
  height: 30PX;
  line-height: 30px;
  margin-top: 11px;border-style: solid;
  border-radius: 4px;
  border-color: white;
  display: inline-block;
}
.footer{
  width: 100%;
  text-align: center;
  height: 60px;
  margin-top: 30px;
}
.commit{
  width: 50%;
  height: 40px;
  line-height: 35px;
  background-color: #00b4af;
  color: white;
  border-width: 1px;
  border-style: solid;
  border-radius: 4px;
  margin: 0 auto;
}