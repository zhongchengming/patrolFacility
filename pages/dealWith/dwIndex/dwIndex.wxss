.content-cell{
  width: 100%;
  margin-bottom: 30px;

}
.content-cell-right{
    width: calc(100% - 60px);
    float:right;
    height:110px; 
    background-color: white;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
    margin-top: 7px;
    padding-top: 1px;
    margin-right: 10px;
}
.content-cell-left{
  float: left;
  width: 50px;
  height:100%;
  margin-top: 7px;
}
.cell-left-day{
  width: 100%;
  height: 30px;
  line-height: 30px;
  font-size: 15px;
   /* padding-left: 4px; */
  display: inline-block;
  text-align: center;
}
.cell-left-week{
  width: 100%;
  height: 30px;
  line-height: 30px;
  font-size: 15px;
  color: rgb(148, 148, 148);
  display: inline-block;
  text-align: center;

}
.cell-left-yuan{
  margin-left: calc(50% - 4px);
  width:2px;
  height:2px;
  border-radius:50%;
  background-color: orange;
  border:2px solid orange;
}
.cell-left-line{
  margin-left: calc(50% - 2px);
  width:1px;
  background-color: rgb(214, 214, 213);
  margin-top: 5px;
}
.cell-left-label{
   width: 40px;
   height: 30px;
}
.content-cell-item1{
  margin-top: 10px;
  height: 20px;
  margin-left: 10px;
  margin-bottom: 5px; 
}

.item-label{
  /* font-weight: bold; */
  font-size: 15px;
  margin-left: 10px;
}
.item-label-right{
  float: right;
  font-size: 15px;
  margin-right: 10px;
  color: #A0A0A0
}
.content-cell-item2{
  margin-left: 33px;
   color: #A0A0A0;
   height: 30px;
   line-height: 30px;
   font-size: 13px;
}
.data-list{
    margin-bottom: 35px;
}
page{
  background-color: #F6FAFA;
